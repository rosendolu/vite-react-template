    "lint-staged": "13.2.3",
    "postcss": "8.4.26",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.1",
    "sass": "1.63.6",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "vite": "4.4.4"
