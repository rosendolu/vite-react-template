    "antd": "5.7.0",
    "babel-polyfill": "^6.26.0",
    "dayjs": "1.11.9",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "react-router-dom": "6.14.1",
    "sort-by": "^1.2.0",
    "usehooks-ts": "^2.9.1"
